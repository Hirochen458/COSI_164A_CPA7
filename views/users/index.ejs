<h2>Users Table</h2>
<a href="<%=`/users/new` %>"> <Button class="btn btn-primary mt-3 mb-3">Sign Up</Button></a>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Graduation Year</th>
      <th>Major</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Zip Code</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><a href="<%= `/users/${user._id}`%>"><%= user.name %></a></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= user.graduationYear %></td>
      <td><%= user.major %></td>
      <td><%= user.city %></td>
      <td><%= user.state %></td>
      <td><%= user.country %></td>
      <td><%= user.zipCode %></td>
      
      <td>
        <a href="<%=`/users/${user._id}/edit` %>"> Edit </a>
      </td>
      <td>
        <a href="<%= `users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you really gonna leave us')">Delete</a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>